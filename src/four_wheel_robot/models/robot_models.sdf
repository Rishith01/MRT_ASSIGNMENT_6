<?xml version="1.0" ?>
<sdf version="1.8">
  <model name="four_wheel_robot">
    <!-- Robot Base -->
    <link name="base_link">
      <visual name="base_visual">
        <geometry>
          <box>
            <size>0.60 0.35 0.52</size>
          </box>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.5 0.5 0.5 1</diffuse>
          <specular>0.5 0.5 0.5 1</specular>
        </material>
      </visual>
      <collision name="base_collision">
        <geometry>
          <box>
            <size>0.60 0.35 0.52</size>
          </box>
        </geometry>
      </collision>
      <inertial>
        <mass>10.0</mass>
        <inertia>
          <ixx>1.0</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>1.0</iyy>
          <iyz>0.0</iyz>
          <izz>1.0</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Wheels -->
    <!-- Front Left Wheel -->
    <link name="front_left_wheel">
      <visual name="front_left_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
          <specular>0 0 0 1</specular>
        </material>
      </visual>
      <collision name="front_left_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="front_left_wheel_joint" type="continuous">
      <parent>base_link</parent>
      <child>front_left_wheel</child>
      <pose>0.30 0.175 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <effort>10.0</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- Front Right Wheel -->
    <link name="front_right_wheel">
      <visual name="front_right_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
          <specular>0 0 0 1</specular>
        </material>
      </visual>
      <collision name="front_right_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="front_right_wheel_joint" type="continuous">
      <parent>base_link</parent>
      <child>front_right_wheel</child>
      <pose>0.30 -0.175 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <effort>10.0</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- Rear Left Wheel -->
    <link name="rear_left_wheel">
      <visual name="rear_left_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
          <specular>0 0 0 1</specular>
        </material>
      </visual>
      <collision name="rear_left_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="rear_left_wheel_joint" type="continuous">
      <parent>base_link</parent>
      <child>rear_left_wheel</child>
      <pose>-0.30 0.175 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <effort>10.0</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- Rear Right Wheel -->
    <link name="rear_right_wheel">
      <visual name="rear_right_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0 0 0 1</ambient>
          <diffuse>0 0 0 1</diffuse>
          <specular>0 0 0 1</specular>
        </material>
      </visual>
      <collision name="rear_right_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.10</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="rear_right_wheel_joint" type="continuous">
      <parent>base_link</parent>
      <child>rear_right_wheel</child>
      <pose>-0.30 -0.175 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <effort>10.0</effort>
          <velocity>10</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0.1</friction>
        </dynamics>
      </axis>
    </joint>

    <!-- LIDAR -->
    <link name="lidar_link">
      <pose>0 0 0.3 0 0 0</pose>
      <sensor name="lidar_sensor" type="ray">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>20</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>640</samples>
              <resolution>1</resolution>
              <min_angle>-1.5708</min_angle>
              <max_angle>1.5708</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.1</min>
            <max>10.0</max>
            <resolution>0.01</resolution>
          </range>
        </ray>
        <plugin name="gazebo_ros_laser" filename="libgazebo_ros_laser.so">
          <ros>
            <namespace>/four_wheel_robot</namespace>
            <remapping>~/scan:=scan</remapping>
          </ros>
          <frameName>lidar_link</frameName>
        </plugin>
      </sensor>
    </link>

    <!-- Depth Camera -->
    <link name="camera_link">
      <pose>0 0 0.3 0 0 0</pose>
      <sensor name="camera_sensor" type="depth">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>30</update_rate>
        <camera>
          <horizontal_fov>1.047</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.1</near>
            <far>10.0</far>
          </clip>
        </camera>
        <plugin name="gazebo_ros_depth_camera" filename="libgazebo_ros_depth_camera.so">
          <ros>
            <namespace>/four_wheel_robot</namespace>
            <remapping>~/depth_camera/image_raw:=depth_image</remapping>
            <remapping>~/depth_camera/points:=depth_points</remapping>
          </ros>
          <frameName>camera_link</frameName>
        </plugin>
      </sensor>
    </link>

    <!-- Differential Drive Plugin -->
    <plugin name="diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <namespace>/four_wheel_robot</namespace>
        <remapping>cmd_vel:=cmd_vel</remapping>
        <remapping>odom:=odom</remapping>
      </ros>
      <updateRate>50</updateRate>
      <leftJoint>rear_left_wheel_joint</leftJoint>
      <leftJoint>front_left_wheel_joint</leftJoint>
      <rightJoint>rear_right_wheel_joint</rightJoint>
      <rightJoint>front_right_wheel_joint</rightJoint>
      <wheelSeparation>0.35</wheelSeparation>
      <wheelDiameter>0.10</wheelDiameter>
      <wheelTorque>5.0</wheelTorque>
    </plugin>
  </model>
</sdf>

